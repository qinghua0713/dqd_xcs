page {
  box-sizing: border-box;

}

.search-container {
  width: 100%;
  position: relative;
}

.search {
  width: 90%;
  height: 56rpx;
  background-color: #f7f7f7;
  border-radius: 30px;
  text-align: center;
  margin: 0 auto;
  font-size: 23rpx;
  color: #a4a4a4;
  margin-bottom: 6rpx;
}

/* image{
  width: 100%;
