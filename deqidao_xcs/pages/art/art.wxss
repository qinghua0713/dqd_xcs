image{
  width: 100%;
  height: 100%;
}
.tab_title{
  width: 100%;
  height: 104rpx;
  background-color: #ffffff;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.tab_option{
  display: flex;
  flex-direction: column;
  justify-content: center;
  color: #686868;
  align-items: center;
}
.tab_option image{
  padding-bottom: 10rpx;
}
.tab_option:nth-child(1) > image{
  width: 42rpx;
	height: 41rpx;
}
.tab_option:nth-child(2) > image{
  width: 42rpx;
	height: 44rpx;
}
.tab_option text {
  font-size: 23rpx;
  padding-bottom: 6rpx;
}
#myVideo{
  width: 100%;
}