    border-radius: 7rpx;
}

.ys_title {
    font-size: 23rpx;
    color: #29292a;
    border-radius: 7rpx 7rpx 0 0;
    background: #ffffff;
    padding: 19rpx 17rpx 6rpx 17rpx;
}

.ys_title text {
    width: 100%;
    display: inline-block;
    border-bottom: 1px solid #e2e2e2;
}

.ysp_earnings .ys_item:nth-child(2) {
    border-radius: 0 0 7rpx 7rpx;
}

.ys_item {
