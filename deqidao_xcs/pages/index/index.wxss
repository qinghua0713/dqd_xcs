.search-image {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 50%;
  left: 260rpx;

  transform: translateY(-50%);
}

.banner {
  width: 100%;
  height: 658rpx;
  background: #454444;
}

.class_list {
  width: 100%;
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
  padding-top: 50rpx;

}

.list_item {
  width: 25%;
  height: 100rpx;
  display: flex;
  align-items: center;
  flex-direction: column;
  margin-bottom: 34rpx;
}
.list_item:nth-child(1) > image{
  width: 89rpx;
	height: 83rpx;
}
.list_item:nth-child(2) > image{
  width: 90rpx;
	height: 90rpx;
}
.list_item:nth-child(3) > image{
	width: 81rpx;
	height: 79rpx;
}
.list_item:nth-child(4) > image{
	width: 108rpx;
	height: 72rpx;
}
.list_item:nth-child(5) > image{
  width: 81rpx;
	height: 83rpx;
}
.list_item:nth-child(6) > image{
  width: 83rpx;
	height: 82rpx;
}
.list_item:nth-child(7) > image{
	width: 80rpx;
	height: 82rpx;
}
.list_item:nth-child(8) > image{
  width: 78rpx;
	height: 78rpx;
}
