}

.jd_progress {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-top: 30rpx;
    padding-left: 16rpx;
    box-sizing: border-box;
    position: relative;
}

.jd_item {
    display: flex;
    flex-direction: column;
    align-items: center;
    font-size: 12rpx;
    color: #a4a4a4;
    position: relative;
   overflow:visible !important;
}

.jd_item image {
    width: 20rpx;
    height: 28rpx;
    position: absolute;
    top: -29rpx;
    left: 15rpx;

}

.circle {
    position: relative;
    width: 14rpx;
    height: 14rpx;
    border: solid 1rpx #a4a4a4;
    border-radius: 50%;
}

.circleDot::before {
    content: "";
    width: 7rpx;
    height: 7rpx;
    background-color: #a4a4a4;
    border-radius: 50%;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    z-index: 100;

}

.ysp_name {
    font-size: 17rpx;
    color: #29292a;
    padding-bottom: 8rpx;
}

.cp_maoshu {
    overflow-wrap: break-word;
    font-size: 17rpx;
    color: #a4a4a4;
    display: -webkit-box;
    -webkit-line-clamp: 4;
    -webkit-box-orient: vertical;
    overflow: hidden;
}

